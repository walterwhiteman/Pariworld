  "scripts": {
    "dev": "NODE_ENV=development tsx server/src/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cd client && npx vite build --outDir ../dist/public --emptyOutDir",
    "build:server": "esbuild server/src/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push"
  },
